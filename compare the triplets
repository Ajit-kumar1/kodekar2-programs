#include<stdio.h>

int main(){
    int a=0,b=0;
    int arr1[3],arr2[3],i;
    for(i=0;i<3;i++)
        scanf("%d",&arr1[i]);
    for(i=0;i<3;i++)
        scanf("%d",&arr2[i]);
    
    for(int i=0;i<3;i++){
        if(arr1[i]>arr2[i])
            a+= 1;
        else if(arr2[i]>arr1[i])
            b+= 1;
        else
            continue;
    }
    printf("%d %d",a,b);
    return 0;
}
    
